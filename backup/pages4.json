					{
						"question": "Is PayloadCMS difficult to learn if I'm coming from other CMS platforms?",
						"answer": {
							"en": {
								"root": {
									"children": [
										{
											"children": [
												{
													"detail": 0,
													"format": 0,
													"mode": "normal",
													"style": "",
